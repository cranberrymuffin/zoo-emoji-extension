{
  "manifest_version": 3,
  "name": "EmojiZoo",
  "version": "1.0.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "description": "Transform animal text across the web into fun emojis with EmojiZoo.",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["dog-emoji.js"]
    }
  ],
  "homepage_url": "https://cranberrymuffin.github.io"
}
